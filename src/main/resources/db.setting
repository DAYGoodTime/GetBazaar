## db.setting文件
[bz_quick]
#数据库主机地址
host = localhost
#用户名，必须
user = root
#密码，必须，如果密码为空，请填写 pass =
pass = kel123
#数据库端口
port = 3306
#数据库名字bz_quick_status
database = bazzar
#JDBC url，必须
url = jdbc:mysql://${host}:${port}/${database}?useSSL=false&allowPublicKeyRetrieval=true&serverTimezone=UTC

## 可选配置
# 是否在日志中显示执行的SQL
showSql = true
# 是否格式化显示的SQL
formatSql = false
# 是否显示SQL参数
showParams = true
# 打印SQL的日志等级，默认debug，可以是info、warn、error
sqlLevel = debug